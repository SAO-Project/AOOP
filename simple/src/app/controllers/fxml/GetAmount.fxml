<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<AnchorPane prefHeight="268.0" prefWidth="390.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.controllers.GetAmountController">
   <children>
      <VBox layoutX="110.0" prefHeight="400.0" prefWidth="170.0">
         <children>
            <Label fx:id="labelField" prefHeight="89.0" prefWidth="170.0" text="ENTER TEXT" />
            <TextField fx:id="amountTextField" promptText="\$$" />
            <Button mnemonicParsing="false" onAction="#enter" prefHeight="43.0" prefWidth="170.0" text="Enter" />
            <Button fx:id="backButton" layoutX="10.0" layoutY="126.0" mnemonicParsing="false" onAction="#back" prefHeight="43.0" prefWidth="170.0" text="Back" />
         </children>
      </VBox>
   </children>
</AnchorPane>
